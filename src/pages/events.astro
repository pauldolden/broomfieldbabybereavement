---
import Layout from "@layouts/Layout.astro";
---

<Layout title="BBB | Events">
  <div class="w-full bg-yellow-gra-end py-5">
    <h2 class="section-title text-center mb-0">Events</h2>
  </div>
  <div class="section-container py-5">
    <iframe
      src="https://calendar.google.com/calendar/embed?src=threebsweb%40proton.me&ctz=Europe%2FLondon&showTitle=0&showNav=1&showPrint=0&showTabs=1&showCalendars=0&mode=MONTH&height=600&wkst=1&bgcolor=%23ffffff&showDate=0&showTitle=0&showTz=0&showTabs=0&showPrint=0&showCalendars=0&showHeader=0"
      style="border-width:0; width:100%; min-width:100%;"
      height="600"
      frameborder="0"
      scrolling="no"></iframe>
  </div>
</Layout>
